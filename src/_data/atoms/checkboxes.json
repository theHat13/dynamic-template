{
  "globalStyle": "appearance-none transition-all duration-200 font-sans text-gray-700",
  "groupLabel": "Your RPG Combat Style",
  "fieldsetClasses": "checkbox-group",
  "legendClasses": "font-medium text-gray-700 mb-3",
  "groupContainerClasses": "space-y-2",
  "labelClasses": "flex cursor-pointer items-start gap-4",
  "checkboxContainerClasses": "flex items-center",
  "baseClasses": "size-4 rounded-sm",
  "labelTextClasses": "font-medium text-gray-900", 
  "descriptionClasses": "text-sm text-gray-500",
  "errorClasses": "text-red-500",
  "variants": {
    "default": "bg-white border border-gray-400 text-gray-900",
    "hover": "bg-white border border-gray-600 text-gray-900",
    "checked": "bg-gray-500 border border-gray-500 text-white",
    "focus": "bg-white border-2 border-blue-500 text-gray-900 outline-none ring-2 ring-blue-300",
    "disabled": "bg-gray-100 border border-gray-300 text-gray-400 cursor-not-allowed",
    "disabledChecked": "bg-gray-300 border border-gray-400 text-gray-500 cursor-not-allowed"
  },
  "checkboxes": [
    { "id": "option1", "label": "I put all my skill points in charisma.", "value": "charisma" },
    { "id": "option2", "label": "I loot everything... even useless junk.", "value": "loot-hoarder" },
    { "id": "option3", "label": "I talk to NPCs until they run out of dialogue.", "value": "npc-dialogue" },
    { "id": "option4", "label": "I roll for initiative before entering a room.", "value": "initiative" },
    { "id": "option5", "label": "I save before every important decision.", "value": "save-scummer" },
    { "id": "option6", "label": "I once tried to romance a dragon in-game.", "value": "dragon-romancer" }
  ]
}